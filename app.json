{
	"expo": {
		"name": "Ocean King",
		"slug": "ocean_king",
		"privacy": "public",
		"sdkVersion": "36.0.0",
		"platforms": ["ios", "android", "web"],
		"version": "3.0.0",
		"orientation": "portrait",
		"icon": "./assets/ocean_king_logo.png",
		"splash": {
			"image": "./assets/ocean_king.png",
			"resizeMode": "contain",
			"backgroundColor": "#212121"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"package": "pt.fhenriques.ocean_king",
			"versionCode": 3,
			"googleServicesFile": "./google-services.json",
			"config": {
				"googleMobileAdsAppId": "${process.env.GOOGLEMOBILEADSAPPID}",
				"googleSignIn": {
					"apiKey": "${GOOGLE_SIGNIN_APIKEY}"
				}
			}
		}
	}
}
